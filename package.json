{
  "name": "runtime-config-helper",
  "version": "0.0.0-development",
  "private": true,
  "scripts": {
    "precommit": "lint-staged",
    "start": "node dist/server.js",
    "start-dev": "nodemon --inspect=0.0.0.0:9229 dist/server.js",
    "build-watch": "npm run build && npm-run-all -p watch start-dev",
    "build": "npm run build-ts && npm run lint-ts",
    "build-ts": "tsc",
    "lint-ts": "tslint -c tslint.json -p tsconfig.lint.json -t stylish --force",
    "watch": "tsc -w",
    "preimport-vars": "tsc",
    "import-vars": "node dist/import-helper.js",
    "test": "jest",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@google-cloud/rcloadenv": "^0.2.1",
    "@google-cloud/storage": "^2.3.0",
    "@oclif/command": "^1.5.5",
    "@oclif/config": "^1.9.0",
    "@types/bluebird": "^3.5.24",
    "@types/google-cloud__storage": "^1.7.2",
    "@types/request-promise-native": "^1.0.15",
    "add": "^2.0.6",
    "bluebird": "^3.5.3",
    "change-case": "^3.0.2",
    "child-process-promise": "^2.2.1",
    "compression": "^1.7.1",
    "debug": "^3.1.0",
    "dotenv": "^6.1.0",
    "express": "~4.15.5",
    "helmet": "^3.9.0",
    "request": "^2.34",
    "request-promise-native": "^1.0.5",
    "shell-escape": "^0.2.0",
    "yarn": "^1.12.3"
  },
  "devDependencies": {
    "@types/compression": "^0.0.35",
    "@types/debug": "^0.0.30",
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.0.39",
    "@types/helmet": "^0.0.37",
    "@types/jest": "^21.1.5",
    "@types/node": "^8.0.49",
    "husky": "^0.14.3",
    "jest": "^23.0.0",
    "lint-staged": "^4.3.0",
    "nodemon": "^1.12.1",
    "npm-run-all": "^4.1.1",
    "prettier": "^1.15.2",
    "semantic-release": "^15.12.0",
    "ts-jest": "^23.10.4",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.16.0",
    "tslint-immutable": "^4.9.1",
    "typescript": "^3.0.0"
  },
  "bin": {
    "import-runtimeconfig-from-env": "dist/import-helper.js"
  },
  "jest": {
    "transform": {
      "^.+\\.ts?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/<rootDir>/src/.*|(\\.|/)spec)\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,ts,tsx,css,scss}": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kamshak/secrets-api.git"
  }
}
